#include<iostream>
#include<vector>

int main (){

int sum=0 ;
int prod=1 ;
std::vector<int>number;

number.push_back(2) ;
number.push_back(4) ;
number.push_back(3);
number.push_back(6);
number.push_back(1);
number.push_back(9) ;

for(int i=0 ;i<number.size();i++){

  if(number[i]%2==0){

    sum+=number[i] ;
  }else{
    prod*=number[i];
  }

}

std::cout<<"sum of number is "<<sum<<"\n";

std::cout<<"product of odd numbers is"<<prod<<"\n" ;

return 0 ;  
}
